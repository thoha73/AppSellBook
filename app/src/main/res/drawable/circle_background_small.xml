<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient android:startColor="#84D797" android:endColor="#AD379A" android:angle="1"/>"
            <size android:width="100dp" android:height="100dp"/>
            <stroke android:width="2dp" android:color="#7BAD8F"/>
        </shape>
    </item>

    <item android:left="10dp" android:right="10dp" android:top="10dp" android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/>
            <size android:width="70dp" android:height="70dp"/>
            <stroke android:width="1dp" android:color="#2D8763"/>
        </shape>
    </item>
</layer-list>
